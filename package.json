{
  "name": "thymeleaf-preview-editor",
  "version": "1.3.0",
  "description": "Thymeleaf Editor with Preview",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": " https://github.com/giraone/thymeleaf-preview-editor"
  },
  "engines": {
    "node": ">=16"
